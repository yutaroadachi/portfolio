{
  "name": "portfolio",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@8.10.2",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "typecheck": "tsc --noEmit",
    "lint": "next lint --max-warnings 0 --dir src",
    "lint:fix": "next lint --fix --dir src",
    "format": "prettier --check \"./src/**/*.{ts,tsx}\"",
    "format:write": "prettier --write \"./src/**/*.{ts,tsx}\"",
    "test": "jest",
    "test:e2e": "playwright test",
    "bundle-analyze": "ANALYZE=true pnpm build"
  },
  "dependencies": {
    "client-only": "0.0.1",
    "clsx": "2.0.0",
    "microcms-js-sdk": "2.7.0",
    "next": "14.0.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "4.11.0",
    "rss-parser": "3.13.0",
    "server-only": "0.0.1"
  },
  "devDependencies": {
    "@babel/core": "7.23.3",
    "@next/bundle-analyzer": "14.0.1",
    "@playwright/test": "1.39.0",
    "@testing-library/react": "14.1.0",
    "@types/jest": "29.5.8",
    "@types/node": "18.16.16",
    "@types/react": "18.2.37",
    "@typescript-eslint/eslint-plugin": "6.9.1",
    "@typescript-eslint/parser": "6.9.1",
    "autoprefixer": "10.4.16",
    "babel-loader": "9.1.3",
    "eslint": "8.53.0",
    "eslint-config-next": "14.0.1",
    "eslint-config-prettier": "9.0.0",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "postcss": "8.4.31",
    "prettier": "3.0.3",
    "tailwindcss": "3.3.5",
    "typescript": "5.2.2"
  }
}
