{
  "name": "portfolio",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@8.15.8",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "typecheck": "tsc --noEmit",
    "lint": "next lint --max-warnings 0 --dir src",
    "lint:fix": "next lint --fix --dir src",
    "format": "prettier --check \"./src/**/*.{ts,tsx}\"",
    "format:write": "prettier --write \"./src/**/*.{ts,tsx}\"",
    "test": "jest",
    "test:e2e": "playwright test",
    "bundle-analyze": "ANALYZE=true pnpm build"
  },
  "dependencies": {
    "client-only": "0.0.1",
    "clsx": "2.1.1",
    "microcms-js-sdk": "3.1.2",
    "next": "14.2.4",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-icons": "5.2.1",
    "rss-parser": "3.13.0",
    "server-only": "0.0.1"
  },
  "devDependencies": {
    "@babel/core": "7.24.8",
    "@next/bundle-analyzer": "14.2.4",
    "@playwright/test": "1.45.1",
    "@testing-library/react": "16.0.0",
    "@types/jest": "29.5.12",
    "@types/node": "18.19.15",
    "@types/react": "18.3.3",
    "@typescript-eslint/eslint-plugin": "7.15.0",
    "@typescript-eslint/parser": "7.15.0",
    "autoprefixer": "10.4.19",
    "babel-loader": "9.1.3",
    "eslint": "8.57.0",
    "eslint-config-next": "14.2.4",
    "eslint-config-prettier": "9.1.0",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "postcss": "8.4.39",
    "prettier": "3.3.2",
    "tailwindcss": "3.4.4",
    "typescript": "5.5.3"
  }
}
