{
  "name": "portfolio",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@8.6.10",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "typecheck": "tsc --noEmit",
    "lint": "next lint --max-warnings 0 --dir src",
    "lint:fix": "next lint --fix --dir src",
    "format": "prettier --check \"./src/**/*.{ts,tsx}\"",
    "format:write": "prettier --write \"./src/**/*.{ts,tsx}\"",
    "test": "jest",
    "test:e2e": "playwright test",
    "bundle-analyze": "ANALYZE=true pnpm build"
  },
  "dependencies": {
    "client-only": "0.0.1",
    "clsx": "2.0.0",
    "microcms-js-sdk": "2.5.0",
    "next": "13.4.12",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "4.10.1",
    "rss-parser": "3.13.0",
    "server-only": "0.0.1"
  },
  "devDependencies": {
    "@babel/core": "7.22.9",
    "@next/bundle-analyzer": "13.4.12",
    "@playwright/test": "1.36.2",
    "@testing-library/react": "14.0.0",
    "@types/jest": "29.5.3",
    "@types/node": "18.16.16",
    "@types/react": "18.2.17",
    "@typescript-eslint/eslint-plugin": "6.1.0",
    "@typescript-eslint/parser": "6.1.0",
    "autoprefixer": "10.4.14",
    "babel-loader": "9.1.3",
    "eslint": "8.45.0",
    "eslint-config-next": "13.4.12",
    "eslint-config-prettier": "8.8.0",
    "jest": "29.6.2",
    "jest-environment-jsdom": "29.6.2",
    "postcss": "8.4.27",
    "prettier": "3.0.0",
    "tailwindcss": "3.3.3",
    "typescript": "5.1.6"
  }
}
