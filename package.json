{
  "name": "portfolio",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "typecheck": "tsc --noEmit",
    "lint": "next lint --dir src",
    "lint:fix": "next lint --fix --dir src",
    "format": "prettier --check \"./src/**/*.{ts,tsx}\"",
    "format:write": "prettier --write \"./src/**/*.{ts,tsx}\"",
    "test": "jest",
    "test:e2e": "playwright test",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "g": "HYGEN_TMPLS=hygen hygen",
    "gen:theme-typings": "chakra-cli tokens ./src/theme/index.ts",
    "postinstall": "yarn gen:theme-typings",
    "bundle-analyze": "ANALYZE=true yarn build"
  },
  "dependencies": {
    "@chakra-ui/icons": "2.0.13",
    "@chakra-ui/react": "2.4.3",
    "@emotion/react": "11.10.5",
    "@emotion/styled": "11.10.5",
    "@vercel/og": "0.0.21",
    "framer-motion": "7.10.2",
    "microcms-js-sdk": "2.3.2",
    "next": "13.0.6",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "4.7.1",
    "rss-parser": "3.12.0"
  },
  "devDependencies": {
    "@babel/core": "7.20.5",
    "@chakra-ui/cli": "2.2.0",
    "@chakra-ui/storybook-addon": "4.0.14",
    "@next/bundle-analyzer": "13.0.6",
    "@playwright/test": "1.28.1",
    "@storybook/addon-actions": "6.5.14",
    "@storybook/addon-essentials": "6.5.14",
    "@storybook/addon-links": "6.5.14",
    "@storybook/builder-webpack5": "6.5.14",
    "@storybook/manager-webpack5": "6.5.14",
    "@storybook/react": "6.5.14",
    "@testing-library/react": "13.4.0",
    "@types/jest": "29.2.4",
    "@types/node": "16.18.9",
    "@types/react": "18.0.26",
    "babel-loader": "9.1.0",
    "chromatic": "6.12.0",
    "eslint": "8.29.0",
    "eslint-config-next": "13.0.6",
    "eslint-config-prettier": "8.5.0",
    "hygen": "6.2.11",
    "jest": "29.3.1",
    "jest-environment-jsdom": "29.3.1",
    "prettier": "2.8.1",
    "storybook-addon-next": "1.7.1",
    "typescript": "4.9.4"
  }
}
