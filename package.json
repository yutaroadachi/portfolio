{
  "name": "portfolio",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "18"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "typecheck": "tsc --noEmit",
    "lint": "next lint --max-warnings 0 --dir app --dir src",
    "lint:fix": "next lint --fix --dir app --dir src",
    "format": "prettier --check \"./{app,src}/**/*.{ts,tsx}\"",
    "format:write": "prettier --write \"./{app,src}/**/*.{ts,tsx}\"",
    "test": "jest",
    "test:e2e": "playwright test",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "bundle-analyze": "ANALYZE=true yarn build"
  },
  "dependencies": {
    "@next/font": "13.1.6",
    "@vercel/og": "0.0.27",
    "client-only": "0.0.1",
    "clsx": "1.2.1",
    "microcms-js-sdk": "2.3.2",
    "next": "13.1.6",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "4.7.1",
    "rss-parser": "3.12.0",
    "server-only": "0.0.1"
  },
  "devDependencies": {
    "@babel/core": "7.20.12",
    "@next/bundle-analyzer": "13.1.6",
    "@playwright/test": "1.30.0",
    "@storybook/addon-actions": "6.5.16",
    "@storybook/addon-essentials": "6.5.16",
    "@storybook/addon-links": "6.5.16",
    "@storybook/builder-webpack5": "6.5.16",
    "@storybook/manager-webpack5": "6.5.16",
    "@storybook/react": "6.5.16",
    "@testing-library/react": "13.4.0",
    "@types/jest": "29.4.0",
    "@types/node": "18.11.18",
    "@types/react": "18.0.27",
    "@typescript-eslint/eslint-plugin": "5.50.0",
    "@typescript-eslint/parser": "5.50.0",
    "autoprefixer": "10.4.13",
    "babel-loader": "9.1.2",
    "chromatic": "6.15.0",
    "eslint": "8.33.0",
    "eslint-config-next": "13.1.6",
    "eslint-config-prettier": "8.6.0",
    "jest": "29.4.1",
    "jest-environment-jsdom": "29.4.1",
    "postcss": "8.4.21",
    "prettier": "2.8.3",
    "storybook-addon-next": "1.7.1",
    "tailwindcss": "3.2.4",
    "typescript": "4.9.5"
  }
