{
  "name": "portfolio",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "18"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "typecheck": "tsc --noEmit",
    "lint": "next lint --max-warnings 0 --dir src",
    "lint:fix": "next lint --fix --dir src",
    "format": "prettier --check \"./src/**/*.{ts,tsx}\"",
    "format:write": "prettier --write \"./src/**/*.{ts,tsx}\"",
    "test": "jest",
    "test:e2e": "playwright test",
    "bundle-analyze": "ANALYZE=true yarn build"
  },
  "dependencies": {
    "client-only": "0.0.1",
    "clsx": "1.2.1",
    "microcms-js-sdk": "2.3.3",
    "next": "13.3.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "4.8.0",
    "rss-parser": "3.13.0",
    "server-only": "0.0.1"
  },
  "devDependencies": {
    "@babel/core": "7.21.4",
    "@next/bundle-analyzer": "13.3.1",
    "@playwright/test": "1.32.3",
    "@testing-library/react": "14.0.0",
    "@types/jest": "29.5.1",
    "@types/node": "18.16.0",
    "@types/react": "18.0.38",
    "@typescript-eslint/eslint-plugin": "5.59.0",
    "@typescript-eslint/parser": "5.59.0",
    "autoprefixer": "10.4.14",
    "babel-loader": "9.1.2",
    "chromatic": "6.17.3",
    "eslint": "8.39.0",
    "eslint-config-next": "13.3.1",
    "eslint-config-prettier": "8.8.0",
    "jest": "29.5.0",
    "jest-environment-jsdom": "29.5.0",
    "postcss": "8.4.23",
    "prettier": "2.8.8",
    "tailwindcss": "3.3.1",
    "typescript": "5.0.4"
  }
}
